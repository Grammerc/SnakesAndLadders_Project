#include <stdio.h>

int main(void) {
    // Clear the screen (optional)
    printf("\033[2J\033[H");

    // === SNAKES (Bright Green) ===
    printf("\033[1;32m"); 
    printf("   _____             _             \n");
    printf("  / ____|           | |            \n");
    printf(" | (___  _ __   __ _| | _____  ___ \n");
    printf("  \\___ \\| '_ \\ / _` | |/ / _ \\/ __|\n");
    printf("  ____) | | | | (_| |   <  __/\\__ \\\n");
    printf(" |_____/|_| |_|\\__,_|_|\\_\\___||___/\n");
    printf("                                    \n");
    printf("                                    \n");

    // === AND (Bright Yellow) ===
    printf("\033[1;33m");
    printf("                  _               \n");
    printf("                 | |              \n");
    printf("   __ _ _ __   __| |              \n");
    printf("  / _` | '_ \\ / _` |              \n");
    printf(" | (_| | | | | (_| |              \n");
    printf("  \\__,_|_| |_|\\__,_|              \n");
    printf("                                  \n");
    printf("                                  \n");

    // === LADDERS (Bright Cyan) ===
    printf("\033[1;36m");
    printf("  _               _     _               \n");
    printf(" | |             | |   | |              \n");
    printf(" | |     __ _  __| | __| | ___ _ __ ___ \n");
    printf(" | |    / _` |/ _` |/ _` |/ _ \\ '__/ __|\n");
    printf(" | |___| (_| | (_| | (_| |  __/ |  \\__ \\\n");
    printf(" |______\\__,_|\\__,_|\\__,_|\\___|_|  |___/\n");

    // Reset colors
    printf("\033[0m");

    return 0;
}
